{
  "name": "me-website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "pnpm run \"/^fix:.*/\"",
    "fix:eslint": "next lint --fix",
    "fix:prettier": "prettier --ignore-unknown --cache --write .",
    "lint": "pnpm run \"/^lint:.*/\"",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --ignore-unknown --cache --check .",
    "lint:types": "tsc --pretty",
    "start": "next start"
  },
  "dependencies": {
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/node": "20.6.1",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "postcss": "8.4.29",
    "prettier": "3.0.3",
    "prettier-plugin-jsdoc": "1.0.2",
    "prettier-plugin-tailwindcss": "0.5.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
}
